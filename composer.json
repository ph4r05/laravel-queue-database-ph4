{
  "name": "ph4r05/laravel-queue-database-ph4",
  "description": "Laravel Database queue driver with optimistic locking",
  "version": "dev-master",
  "license": "MIT",
  "authors": [
    {
      "name": "Dusan Klinec (ph4r05)",
      "email": "ph4r05@gmail.com",
      "homepage": "https://ph4r05.deadcode.me"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/support": "^7.0",
    "illuminate/database": "^7.0",
    "illuminate/queue": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": " ^7.0",
    "mockery/mockery": "~1.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "ph4r05\\LaravelDatabasePh4\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ph4r05\\LaravelDatabasePh4\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ph4r05\\LaravelDatabasePh4\\LaravelQueuePh4ServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

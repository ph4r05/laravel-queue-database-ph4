{
  "name": "ph4r05/laravel-queue-database-ph4",
  "description": "Laravel Database queue driver with optimistic locking",
  "license": "MIT",
  "authors": [
    {
      "name": "Dusan Klinec (ph4r05)",
      "email": "ph4r05@gmail.com",
      "homepage": "https://ph4r05.deadcode.me"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/database": "5.5.* || 5.6.* || 5.7.* || 5.8.*|| 6.*",
    "illuminate/support": "5.5.* || 5.6.* || 5.7.* || 5.8.*|| 6.*",
    "illuminate/queue": "5.5.* || 5.6.* || 5.7.* || 5.8.*|| 6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "illuminate/events": "5.5.* || 5.6.* || 5.7.* || 5.8.*|| 6.*",
    "mockery/mockery": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "ph4r05\\LaravelDatabasePh4\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ph4r05\\LaravelDatabasePh4\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.4-dev"
    },
    "laravel": {
      "providers": [
        "ph4r05\\LaravelDatabasePh4\\LaravelQueuePh4ServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
